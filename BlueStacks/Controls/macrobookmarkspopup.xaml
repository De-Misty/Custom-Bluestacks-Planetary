<controls:UserControl
	x:Class="BlueStacks.BlueStacksUI.MacroBookmarksPopup"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:BlueStacks.BlueStacksUI"
	xmlns:Common="clr-namespace:BlueStacks.Common;assembly=HD-Common"
	x:Name="mMacroBookmarksPopup"
	Width="140"
	MaxWidth="160"
	MinHeight="36"
	Margin="0"
	xmlns:controls="clr-namespace:System.Windows.Controls;assembly=PresentationFramework"
	Background="{Binding ColorModel.[SidebarBackground]}"
	DataContext="{Binding Source='{x:Static Common:BlueStacksUIBinding.Instance}'}"
	Loaded="MMacroBookmarksPopup_Loaded">
	<Grid
		x:Name="mGrid"
		Margin="0,6">
		<StackPanel
			x:Name="mMainStackPanel"
			Orientation="Vertical" />
	</Grid>
</controls:UserControl>
